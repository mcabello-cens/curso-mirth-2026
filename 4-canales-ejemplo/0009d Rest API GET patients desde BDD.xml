        <script>var dbConn;
var result;
var patientsArray = [];

try {
	dbConn = DatabaseConnectionFactory.createDatabaseConnection(&apos;com.mysql.cj.jdbc.Driver&apos;,&apos;jdbc:mysql://localhost:3306/curso-mirth?useSSL=false&amp;serverTimezone=UTC&apos;,&apos;root&apos;,&apos;plop&apos;);
	// var params = new Packages.java.util.ArrayList();
	//params.add($(&apos;Date of birth param&apos;));
	// result = dbConn.executeCachedQuery(&apos;select * from patient where dateofbirth &gt; ? &apos;, params);
	result = dbConn.executeCachedQuery(&apos;select * from patient&apos;);

	while (result.next()) {
		logger.info(&apos;there are results&apos;);
		var firstname = result.getString(&apos;nombre&apos;);
		var lastname = result.getString(&apos;apellido1&apos;);
		var dob = result.getString(&apos;nacimiento&apos;);
		var patientObject = {firstname: firstname, lastname: lastname, date_of_birth: dob};
		patientsArray.push(patientObject);
	}
	channelMap.put(&apos;responseContentType&apos;, &apos;application/json&apos;);
	channelMap.put(&apos;responseCode&apos;, &apos;200&apos;);
	return JSON.stringify(patientsArray);
} catch (e) {
	logger.info(e);
	return [];
} finally {
	if (dbConn) {
		dbConn.close();
	}
}</script>
