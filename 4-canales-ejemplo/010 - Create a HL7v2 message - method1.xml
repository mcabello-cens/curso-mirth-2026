          <script>/**
 * ============================================================
 * Mirth Connect 4.5.2 (JavaScript Rhino)
 * Transformer: JSON → HL7 v2 ADT^A01
 *
 * Entrada:
 *  - JSON recibido en:
 *      a) channelMap[&quot;payloadJson&quot;]  (si existe)
 *      b) msg (payload de entrada del Source)
 *
 * Salida:
 *  - HL7 v2 ADT_A01 en channelMap[&quot;outHL7&quot;]
 *
 * Segmentos generados:
 *  - MSH
 *  - EVN
 *  - PID
 *  - PV1
 *
 * ============================================================
 */


/* ============================================================
 * CONFIGURACIÓN GENERAL (constantes del canal)
 * Idealmente esto debiera venir desde Configuration Map
 * ============================================================
 */
var CFG = {

  // Identificación del mensaje HL7
  sendingApp: &quot;MIRTH&quot;,
  sendingFacility: &quot;CLINIC&quot;,
  receivingApp: &quot;EHR&quot;,
  receivingFacility: &quot;HOSPITAL&quot;,
  hl7Version: &quot;2.5.1&quot;,

  // Identificador del paciente (PID-3)
  patientIdAssigningAuthority: &quot;CLINIC&quot;,
  patientIdType: &quot;MR&quot;,

  // Identificador de la visita (PV1-19)
  visitAssigningAuthority: &quot;CLINIC&quot;,
  visitIdType: &quot;VN&quot;,

  // Clase del paciente (PV1-2)
  // E = Emergency, I = Inpatient, O = Outpatient
  patientClass: &quot;E&quot;,

  // Ubicación por defecto (PV1-3)
  defaultLocationFacility: &quot;MAIN&quot;,
  defaultLocationBuilding: &quot;B1&quot;,
  defaultLocationPointOfCare: &quot;ER&quot;,
  defaultLocationRoom: &quot;&quot;,
  defaultLocationBed: &quot;&quot;,

  // Prefijo del Message Control ID
  msgControlPrefix: &quot;ADT&quot;
};


/* ============================================================
 * FUNCIONES AUXILIARES (helpers)
 * ============================================================
 */

/**
 * nvl
 * Devuelve un valor por defecto si el valor es null, undefined o vacío.
 */
function nvl(value, defaultValue) {
  return (value === null || value === undefined || value === &quot;&quot;)
    ? defaultValue
    : value;
}

/**
 * pad2
 * Asegura dos dígitos (para fechas/horas HL7)
 */
function pad2(n) {
  return (n &lt; 10 ? &quot;0&quot; : &quot;&quot;) + n;
}

/**
 * tsNow
 * Genera timestamp HL7: yyyyMMddHHmmss
 */
function tsNow() {
  var d = new Date();
  return &quot;&quot;
    + d.getFullYear()
    + pad2(d.getMonth() + 1)
    + pad2(d.getDate())
    + pad2(d.getHours())
    + pad2(d.getMinutes())
    + pad2(d.getSeconds());
}

/**
 * toHL7Date
 * Convierte fecha ISO (YYYY-MM-DD) a HL7 (YYYYMMDD)
 */
function toHL7Date(isoDate) {
  if (!isoDate) return &quot;&quot;;
  return String(isoDate).replace(/-/g, &quot;&quot;);
}

/**
 * escapeHL7
 * Escapa caracteres HL7 básicos que romperían el mensaje
 * (implementación mínima y consciente)
 */
function escapeHL7(value) {
  if (value === null || value === undefined) return &quot;&quot;;
  return String(value)
    .replace(/\|/g, &quot;\\F\\&quot;)
    .replace(/\^/g, &quot;\\S\\&quot;);
}

/**
 * genMsgControlId
 * Genera un Message Control ID razonablemente único
 */
function genMsgControlId(prefix) {
  var r = Math.floor(Math.random() * 100000);
  return prefix + tsNow() + &quot;-&quot; + r;
}

/**
 * genderToHL7
 * Normaliza sexo a valores HL7 válidos
 */
function genderToHL7(gender) {
  gender = (gender || &quot;&quot;).toUpperCase();
  if (gender === &quot;M&quot; || gender === &quot;F&quot; || gender === &quot;O&quot; || gender === &quot;U&quot;) {
    return gender;
  }
  return &quot;U&quot;;
}

/**
 * buildXPN
 * Construye un campo XPN (nombre del paciente)
 * PID-5
 */
function buildXPN(lastName, firstName) {
  return escapeHL7(nvl(lastName, &quot;&quot;))
    + &quot;^&quot;
    + escapeHL7(nvl(firstName, &quot;&quot;))
    + &quot;^^^^&quot;;
}

/**
 * buildXCN
 * Construye un campo XCN (médico tratante)
 * PV1-7
 */
function buildXCN(id, lastName, firstName) {
  return escapeHL7(nvl(id, &quot;&quot;))
    + &quot;^&quot;
    + escapeHL7(nvl(lastName, &quot;&quot;))
    + &quot;^&quot;
    + escapeHL7(nvl(firstName, &quot;&quot;))
    + &quot;^^^^^&quot;;
}

/**
 * buildPL
 * Construye un campo PL (Patient Location)
 * PV1-3
 */
function buildPL(pointOfCare, room, bed, facility, building) {
  return escapeHL7(nvl(pointOfCare, &quot;&quot;)) + &quot;^&quot;
       + escapeHL7(nvl(room, &quot;&quot;)) + &quot;^&quot;
       + escapeHL7(nvl(bed, &quot;&quot;)) + &quot;^&quot;
       + escapeHL7(nvl(facility, &quot;&quot;)) + &quot;^^^&quot;
       + escapeHL7(nvl(building, &quot;&quot;)) + &quot;^&quot;;
}


/* ============================================================
 * LECTURA DEL PAYLOAD DE ENTRADA
 * ============================================================
 */

/**
 * 1) Intentamos leer el JSON desde channelMap (si alguien lo dejó ahí)
 * 2) Si no existe, usamos msg (payload del Source Connector)
 */
var raw = channelMap.get(&quot;payloadJson&quot;);
if (raw === null || raw === undefined || String(raw) === &quot;&quot;) {
  raw = msg;
}

/**
 * Parseo seguro del JSON
 */
var payload;
try {
  payload = JSON.parse(String(raw));
} catch (e) {
  logger.error(&quot;JSON inválido recibido: &quot; + String(raw));
  throw &quot;No se pudo parsear JSON de entrada: &quot; + e;
}


/* ============================================================
 * EXTRACCIÓN DE DATOS DEL JSON
 * ============================================================
 */

var event     = nvl(payload.event, &quot;A01&quot;);
var patient   = payload.patient || {};
var visit     = payload.visit || {};
var attending = visit.attending || {};


/* ============================================================
 * CONSTRUCCIÓN DEL MENSAJE HL7
 * ============================================================
 */

// Separadores HL7
var fieldSep = &quot;|&quot;;
var encChars = &quot;^~\\&amp;&quot;;

// Fechas y control
var msh7 = tsNow();
var msgControlId = genMsgControlId(CFG.msgControlPrefix);


/**
 * MSH – Message Header
 */
var msh =
  &quot;MSH&quot; + fieldSep + encChars
  + fieldSep + CFG.sendingApp
  + fieldSep + CFG.sendingFacility
  + fieldSep + CFG.receivingApp
  + fieldSep + CFG.receivingFacility
  + fieldSep + msh7
  + fieldSep + &quot;&quot;
  + fieldSep + (&quot;ADT^&quot; + event + &quot;^ADT_A01&quot;)
  + fieldSep + msgControlId
  + fieldSep + &quot;P&quot;
  + fieldSep + CFG.hl7Version;


/**
 * EVN – Event Type
 */
var evn =
  &quot;EVN&quot; + fieldSep
  + event
  + fieldSep + msh7;


/**
 * PID – Patient Identification
 */
var pid =
  &quot;PID&quot; + fieldSep
  + &quot;1&quot;
  + fieldSep + &quot;&quot;
  + fieldSep
    + escapeHL7(nvl(patient.id, &quot;&quot;)) + &quot;^^^&quot;
    + CFG.patientIdAssigningAuthority + &quot;^&quot;
    + CFG.patientIdType
  + fieldSep + &quot;&quot;
  + fieldSep + buildXPN(patient.lastName, patient.firstName)
  + fieldSep + &quot;&quot;
  + fieldSep + toHL7Date(patient.birthDate)
  + fieldSep + genderToHL7(patient.gender);


/**
 * PV1 – Patient Visit
 */
var pv1 =
  &quot;PV1&quot; + fieldSep
  + &quot;1&quot;
  + fieldSep + CFG.patientClass
  + fieldSep + buildPL(
      nvl(visit.location, CFG.defaultLocationPointOfCare),
      CFG.defaultLocationRoom,
      CFG.defaultLocationBed,
      CFG.defaultLocationFacility,
      CFG.defaultLocationBuilding
    )
  + fieldSep + &quot;&quot;
  + fieldSep + &quot;&quot;
  + fieldSep + &quot;&quot;
  + fieldSep + buildXCN(attending.id, attending.lastName, attending.firstName)
  + fieldSep + &quot;&quot; + fieldSep + &quot;&quot; + fieldSep + &quot;&quot; + fieldSep + &quot;&quot;
  + fieldSep + &quot;&quot; + fieldSep + &quot;&quot; + fieldSep + &quot;&quot; + fieldSep + &quot;&quot;
  + fieldSep + &quot;&quot; + fieldSep + &quot;&quot;
  + fieldSep
    + escapeHL7(nvl(visit.visitNumber, &quot;&quot;)) + &quot;^^^&quot;
    + CFG.visitAssigningAuthority + &quot;^&quot;
    + CFG.visitIdType;


/* ============================================================
 * MENSAJE FINAL
 * ============================================================
 */

var hl7 =
  msh + &quot;\r&quot;
  + evn + &quot;\r&quot;
  + pid + &quot;\r&quot;
  + pv1 + &quot;\r&quot;;

// Publicar resultado
channelMap.put(&quot;outHL7&quot;, hl7);
channelMap.put(&quot;msh10&quot;, msgControlId);
channelMap.put(&quot;msh7&quot;, msh7);


// Opcional: reemplazar payload de salida
// msg = hl7;</script>
