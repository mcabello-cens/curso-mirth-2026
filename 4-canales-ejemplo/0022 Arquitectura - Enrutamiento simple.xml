          <sequenceNumber>2</sequenceNumber>
          <enabled>false</enabled>
          <script>// equivalente a Destination Set Filter
var adt = msg[&apos;MSH&apos;][&apos;MSH.9&apos;][&apos;MSH.9.1&apos;].toString();
var adt_type = msg[&apos;MSH&apos;][&apos;MSH.9&apos;][&apos;MSH.9.2&apos;].toString();

if (adt == &quot;ADT&quot;) {
	if (adt_type == &quot;A01&quot;) {
		destinationSet.remove([2]);
		return msg;
	}
	if (adt_type == &quot;A04&quot;) {
		destinationSet.remove([1]);
		return msg;
	}
	return false;
}
else {
	logger.info(&quot;No es un mensaje ADT&quot;);
	return false;
	}</script>
