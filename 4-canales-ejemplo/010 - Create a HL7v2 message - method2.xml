        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="4.5.2">
          <name>create HL7 v2 message</name>
          <sequenceNumber>5</sequenceNumber>
          <enabled>false</enabled>
          <script>try {
	var segMSH = msh.toString();
	var segPID = pid.toString();
	var segEVN = evn.toString();
	var segPV1 = pv1.toString();

	logger.info(&quot;segMSH=&quot; + segMSH);
	logger.info(&quot;segPID=&quot; + segPID);
	logger.info(&quot;segEVN=&quot; + segEVN);
	logger.info(&quot;segPV1=&quot; + segPV1);
	
	// HL7 standard segment delimiter is \r (carriage return)
	var finalMessage = segMSH + &quot;\r&quot; + segEVN + &quot;\r&quot; + segPID + &quot;\r&quot; + segPV1 + &quot;\r&quot;;
	
	channelMap.put(&quot;finalMessage&quot;, finalMessage);
	
	var dateString = DateUtil.getCurrentDate(&quot;yyyyMMddHHmm&quot;);
	channelMap.put(&quot;fileName&quot;, dateString);
	
	logger.info(&quot;finalMessage=&quot; + finalMessage);
	
	msg = finalMessage;
} catch (e) { logger.error(e) }</script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="4.5.2">
          <name>create HL7v2 message</name>
          <sequenceNumber>6</sequenceNumber>
          <enabled>true</enabled>
          <script>// se crea un contenedor ER7, se agregan los segmentos y luego se asigna a msg
// importante que el conector de salida este configurado como Hl7v2, de este modo el mismo Mirth lo
// serializa en un ER7

var xml = &lt;HL7Message/&gt;; // contenedor XML simple

logger.info(&quot;xml=&quot; +  xml);
xml.appendChild(msh);
xml.appendChild(evn);
xml.appendChild(pid);
xml.appendChild(pv1);

// asigna al mensaje transformado
msg = xml;

logger.info(msg);

