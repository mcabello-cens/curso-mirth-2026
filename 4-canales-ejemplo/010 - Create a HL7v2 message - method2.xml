          <name>fill MSH segment</name>
          <sequenceNumber>1</sequenceNumber>
          <enabled>true</enabled>
          <script>// Helpers
function hl7Now() {
  // HL7 TS: yyyyMMddHHmmss (en timezone del servidor)
  //return DateUtil.formatDate(new Date(), &apos;yyyyMMddHHmmss&apos;);
  return &apos;202601151125&apos;;
}

function escHL7(s) {
  // Escapado mínimo para texto (si vas a meter acentos y cosas raras, igual funciona)
  if (s === null || s === undefined) return &apos;&apos;;
  return String(s);
}



try {
	
	msh[&apos;MSH.1&apos;] = &apos;|&apos;;
	msh[&apos;MSH.2&apos;] = &apos;^~\\&amp;&apos;;
	
	// Sending/Receiving
	msh[&apos;MSH.3&apos;] = &apos;SENDER-APP&apos;;        // Sending Application
	msh[&apos;MSH.4&apos;] = &apos;SENDER-FACILITY&apos;;       // Sending Facility
	msh[&apos;MSH.5&apos;] = &apos;RECEIVER-APP&apos;;          // Receiving Application
	msh[&apos;MSH.6&apos;] = &apos;RECEIVER-FACILITY&apos;;     // Receiving Facility
	
	// Date/Time
	msh[&apos;MSH.7&apos;] = hl7Now();
	
	// Message Type ADT^A01
	msh[&apos;MSH.9&apos;][&apos;MSH.9.1&apos;] = &apos;ADT&apos;;
	msh[&apos;MSH.9&apos;][&apos;MSH.9.2&apos;] = &apos;A01&apos;;
	msh[&apos;MSH.9&apos;][&apos;MSH.9.3&apos;] = &apos;ADT_A01&apos;;
	
	// Control / Processing / Version
	
	msh[&apos;MSH.10&apos;][&apos;MSH.10.1&apos;] = &apos;MSG-&apos; + java.util.UUID.randomUUID().toString(); // Message Control ID
	msh[&apos;MSH.11&apos;][&apos;MSH.11.1&apos;] = &apos;P&apos;;    // Processing ID (P=Production, T=Test)
	msh[&apos;MSH.12&apos;][&apos;MSH.12.1&apos;] = &apos;2.5&apos;;  // Version
	
	connectorMap.put(&apos;msh&apos;, msh);
	logger.info(&apos;msh=&apos; + msh);
	
} catch (e) { 
	logger.error(e);
}</script>
