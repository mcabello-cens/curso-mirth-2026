          <script>// Helpers
function hl7Now() {
  // HL7 TS: yyyyMMddHHmmss (en timezone del servidor)
  //return DateUtil.formatDate(new Date(), &apos;yyyyMMddHHmmss&apos;);
  return &apos;202601151125&apos;;
}

function escHL7(s) {
  // Escapado mínimo para texto (si vas a meter acentos y cosas raras, igual funciona)
  if (s === null || s === undefined) return &apos;&apos;;
  return String(s);
}


try {
	// ------------------------------
	// PV1
	// ------------------------------
	// PV1-1 Set ID
	pv1[&apos;PV1.1&apos;][&apos;PV1.1.1&apos;] = &apos;1&apos;;

	// PV1-2 Patient Class (I=Inpatient, O=Outpatient, E=Emergency)
	pv1[&apos;PV1.2&apos;][&apos;PV1.2.1&apos;] = &apos;I&apos;;
	
	// PV1-3 Assigned Patient Location (PL) -&gt; Point of Care^Room^Bed^Facility...
	pv1[&apos;PV1.3&apos;][&apos;PV1.3.1&apos;] = &apos;UCI&apos;; // Point of Care
	pv1[&apos;PV1.3&apos;][&apos;PV1.3.2&apos;] = &apos;101&apos;;       // Room
	pv1[&apos;PV1.3&apos;][&apos;PV1.3.3&apos;] = &apos;1&apos;;       // Bed
	pv1[&apos;PV1.3&apos;][&apos;PV1.3.4&apos;] = &apos;HOSPITAL&apos;;

	// PV1-7 Attending Doctor (XCN) -&gt; ID^Family^Given
	pv1[&apos;PV1.7&apos;][&apos;PV1.7.1&apos;] = &apos;DOC123&apos;;
	pv1[&apos;PV1.7&apos;][&apos;PV1.7.2&apos;] = &apos;GOMEZ&apos;;
	pv1[&apos;PV1.7&apos;][&apos;PV1.7.3&apos;] = &apos;ANA&apos;;
	pv1[&apos;PV1.9&apos;][&apos;PV1.9.1&apos;] = &apos;MED&apos;;
	pv1[&apos;PV1.16&apos;][&apos;PV1.16.1&apos;] = &apos;123456&apos;;
	pv1[&apos;PV1.17&apos;][&apos;PV1.17.1&apos;] = &apos;A0&apos;;
	 
	// PV1-19 Visit Number (CX)
	pv1[&apos;PV1.19&apos;][&apos;PV1.19.1&apos;] = &apos;V0000001&apos;;
	pv1[&apos;PV1.19&apos;][&apos;PV1.19.5&apos;] = &apos;VN&apos;;

	
} catch (e) { 
	logger.error(e);
}</script>
