          <script>// Helpers
function hl7Now() {
  // HL7 TS: yyyyMMddHHmmss (en timezone del servidor)
  //return DateUtil.formatDate(new Date(), &apos;yyyyMMddHHmmss&apos;);
  return &apos;202601151125&apos;;
}

function escHL7(s) {
  // Escapado mínimo para texto (si vas a meter acentos y cosas raras, igual funciona)
  if (s === null || s === undefined) return &apos;&apos;;
  return String(s);
}

try {

	// ------------------------------
	// PID
	// ------------------------------
	// PID-1 Set ID
	pid[&apos;PID.1&apos;] = &apos;1&apos;;
	
	// PID-3 Patient Identifier List (CX) -&gt; ID^AssigningAuthority^IdentifierTypeCode etc.
	// Aquí dejamos un CX simple

	pid[&apos;PID.3&apos;][&apos;PID.3.1&apos;] = &apos;123456&apos;;   // Patient ID
	
	pid[&apos;PID.3&apos;][&apos;PID.3.5&apos;] = &apos;PI&apos;;       // Identifier Type Code
	pid[&apos;PID.3&apos;][&apos;PID.3.4&apos;] = &apos;HOSPITAL&apos;; // Assigning Authority (HD.1)
	pid[&apos;PID.3&apos;][&apos;PID.3.5&apos;] = &apos;MR&apos;;

	// PID-5 Patient Name (XPN) -&gt; Family^Given
	pid[&apos;PID.5&apos;][&apos;PID.5.1&apos;] = escHL7(&apos;PEREZ&apos;);  // Family Name
	pid[&apos;PID.5&apos;][&apos;PID.5.2&apos;] = escHL7(&apos;JUAN&apos;);   // Given Name
	pid[&apos;PID.5&apos;][&apos;PID.5.3&apos;] = escHL7(&apos;CARLOS&apos;);

	// PID-7 Date/Time of Birth
	pid[&apos;PID.7&apos;] = &apos;19850520&apos;;
	
	// PID-8 Administrative Sex
	pid[&apos;PID.8&apos;] = &apos;M&apos;;
	
	// PID-11 Patient Address (XAD) - opcional, ejemplo mínimo
	pid[&apos;PID.11&apos;][&apos;PID.11.1&apos;] = escHL7(&apos;AV. SIEMPREVIVA 123&apos;);
	pid[&apos;PID.11&apos;][&apos;PID.11.3&apos;] = escHL7(&apos;SANTIAGO&apos;);
	pid[&apos;PID.11&apos;][&apos;PID.11.5&apos;] = escHL7(&apos;RM&apos;);
	pid[&apos;PID.11&apos;][&apos;PID.11.7&apos;] = escHL7(&apos;H&apos;);

	
	pid[&apos;PID.13&apos;] = escHl7(&apos;(56)912345678&apos;);
	pid[&apos;PID.16&apos;] = &apos;S&apos;;
	pid[&apos;PID.18&apos;] = escHl7(&apos;12345678-9&apos;)
	
} catch (e) { 
	logger.error(e);
}</script>
