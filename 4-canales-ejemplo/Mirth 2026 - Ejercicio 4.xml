          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script>var mType = msg[&apos;MSH&apos;][&apos;MSH.9&apos;][&apos;MSH.9.1&apos;].toString();

var mEvent = msg[&apos;MSH&apos;][&apos;MSH.9&apos;][&apos;MSH.9.2&apos;].toString();

var messageType = mType + &apos;_&apos; + mEvent;

if (messageType == &apos;ADT_A01&apos; || messageType == &apos;ADT_A08&apos;) {
	channelMap.put(&apos;messageType&apos;, messageType);
	return true;
}

logger.info(&apos;El mensaje ha sido filtrado&apos;);
return false;</script>
