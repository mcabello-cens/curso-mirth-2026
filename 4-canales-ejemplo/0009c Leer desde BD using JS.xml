      <select>var dbConn;
var rs;

try {
	var user = configurationMap.get(&quot;DB.POSTGRES.USER&quot;);
	var password = configurationMap.get(&quot;DB.POSTGRES.PASSWORD&quot;);
	var url = configurationMap.get(&quot;DB.POSTGRES.JDBC.URL&quot;);
	
	dbConn = DatabaseConnectionFactory.createDatabaseConnection(&apos;org.postgresql.Driver&apos;, url, user, password);

    // 2. Define and execute your SQL query
    var sql = &quot;SELECT id, rut, nombre, apellido_paterno, apellido_materno, nacimiento, telefono, email FROM citas.pacientes&quot;; //  WHERE condition = &apos;value&apos;&quot;;
    rs = dbConn.executeCachedQuery(sql); // Executes a SELECT query
    // 2) Convertir ResultSet a List&lt;Map&gt; (lo que el connector espera)
	var rows = new java.util.ArrayList();
	
	while (rs.next()) {
	  var m = new java.util.HashMap();
	  m.put(&quot;id&quot;, rs.getString(&quot;id&quot;));
	  m.put(&quot;rut&quot;, rs.getString(&quot;rut&quot;));
	  m.put(&quot;nombre&quot;, rs.getString(&quot;nombre&quot;));
	  m.put(&quot;apellido_paterno&quot;, rs.getString(&quot;apellido_paterno&quot;));
	  m.put(&quot;apellido_materno&quot;, rs.getString(&quot;apellido_materno&quot;));
	  m.put(&quot;email&quot;, rs.getString(&quot;email&quot;));
	  rows.add(m);
	}
	logger.info(&quot;consulta exitosa&quot;);
	return rows;
	
} finally {
	if (dbConn) { 
		dbConn.close();
		logger.info(&quot;conexion cerrada&quot;);
	}
}</select>
