          <script>// declaro variable objeto y mapeo los valores de origen
var patient = {
	resourceType: &apos;Patient&apos;,
	identifier: [{
		use: &apos;official&apos;,
		value: msg[&apos;PID&apos;][&apos;PID.2&apos;][&apos;PID.2.1&apos;].toString()
		}],
	name: [{
		use: &apos;official&apos;,
		family: msg[&apos;PID&apos;][&apos;PID.5&apos;][&apos;PID.5.1&apos;].toString(),
		given: [ msg[&apos;PID&apos;][&apos;PID.5&apos;][&apos;PID.5.2&apos;].toString() ]
		}],
	gender: convertGender( msg[&apos;PID&apos;][&apos;PID.8&apos;][&apos;PID.8.1&apos;].toString() ),
	birthday: msg[&apos;PID&apos;][&apos;PID.7&apos;][&apos;PID.7.1&apos;].toString()
	}

// muestro el contenido
logger.info(JSON.stringify(patient, null, 2));

// listo para retornar al conector de salida.
msg = patient;





// helper - conversion del sexo en hl7 v2 a FHIR
function convertGender(value) {
	if (value == &apos;M&apos;) return &apos;male&apos;
	if (value == &apos;F&apos;) return &apos;female&apos;
	return &apos;unknow&apos;
}</script>
